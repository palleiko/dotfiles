Xft.dpi: 96
Xft.antialias: 1
Xft.rgba: rgb
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hinting: 1
Xft.hintstyle: hintfull

Xcursor.theme: Neutral

!!THEME
!!START
! Base16 Google Dark
! Scheme: Seth Wright (http://sethawright.com)

#define base00 #1d1f21
#define base01 #282a2e
#define base02 #373b41
#define base03 #969896
#define base04 #b4b7b4
#define base05 #c5c8c6
#define base06 #e0e0e0
#define base07 #ffffff
#define base08 #cc342b
#define base09 #f96a38
#define base0A #fba922
#define base0B #198844
#define base0C #3971ed
#define base0D #3971ed
#define base0E #a36ac7
#define base0F #3971ed

*foreground:   base05
#ifdef background_opacity
*background:   [background_opacity]base00
#else
*background:   base00
#endif
*cursorColor:  base05

*color0:       base00
*color1:       base08
*color2:       base0B
*color3:       base0A
*color4:       base0D
*color5:       base0E
*color6:       base0C
*color7:       base05

*color8:       base03
*color9:       base08
*color10:      base0B
*color11:      base0A
*color12:      base0D
*color13:      base0E
*color14:      base0C
*color15:      base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*color16:      base09
*color17:      base0F
*color18:      base01
*color19:      base02
*color20:      base04
*color21:      base06
!!END

! URxvt*font: xft:PragmataProMono Nerd Font:size=11:antialias=true
URxvt*font: xft:PragmataProMono Nerd Font, xft:Noto Emoji:size=8:antialias=true

URxvt.letterSpace: 0

URxvt.keysym.Shift-Up: command:033]720;1007
URxvt.keysym.Shift-Down: command:033]721;1007

URxvt.depth: 32
URxvt*scrollBar: false
URxvt.urgentOnBell: True
URxvt.visualBell: False
URxvt*saveLines: 1024
URxvt*mouseWheelScrollPage: true

URxvt.url-select.launcher: /usr/bin/chromium
URxvt.url-select.underline: true

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.font: PragmataProMono Nerd Font 12
rofi.color-normal: base00, base05, base00, base01, base05
rofi.color-urgent: base00, base08, base00, base08, base05
rofi.color-active: base00, base0A, base00, base01, base0A
rofi.color-window: base00, base03, base08
rofi.separator-style: solid
